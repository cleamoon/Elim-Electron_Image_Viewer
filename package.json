{
  "name": "elim",
  "version": "0.1.0",
  "description": "A minimal image viewer written in Electron and Vue.js",
  "main": "build/main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "clean": "rm -rf node_modules && rm -rf build && mkdir build",
    "build": "node scripts/build.js && electron-builder",
    "build:win": "node scripts/build.js && electron-builder --win",
    "build:mac": "node scripts/build.js && electron-builder --mac",
    "build:linux": "node scripts/build.js && electron-builder --linux"
  },
  "repository": "https://github.com/cleamoon/Elim-Electron_Image_Viewer",
  "author": {
    "name": "Yue Jiao",
    "url": "https://yj0.se"
  },
  "devDependencies": {
    "@types/node": "20.8.7",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "chalk": "4.1.2",
    "chokidar": "3.5.3",
    "electron": "25.2.0",
    "electron-builder": "24.2.1",
    "eslint": "8.0.1",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "15.0.0 || 16.0.0 ",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-vue": "9.17.0",
    "typescript": "*",
    "vite": "4.1.1"
  },
  "dependencies": {
    "vue": "3.2.47"
  }
}